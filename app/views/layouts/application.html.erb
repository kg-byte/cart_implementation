<!DOCTYPE html>
<html>
  <head>
    <title>SetList2008</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <p>Cart: <%= cart.total_count %></p>
    <%= yield %>
     <div class="flash_message">
    <% flash.each do |type, message| %>
        <p><%= message %></p>
    <% end %>
  </body>
</html>
